void forward_solve(DefaultRhs              X,
                   const DefaultBlocks&    DBlocks,
                   const DefaultBlocks&    UBlocks,
                   const DefaultIndexSets& nnzIndSets,
                   const DefaultReordering& reorderingData,
                   DefaultValuePtr         vals);

